<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Homepage"
    android:background="@drawable/mainbg"
    >

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:fontFamily="sans-serif"
        android:text="StudentGo"
        android:textColor="#000000"
        android:textSize="40dp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/b4"
        android:layout_width="204dp"
        android:layout_height="65dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="123dp"
        android:background="#010204"
        android:text="@string/log_out"
        android:textColor="#FFFFFF" />

    <ImageButton
        android:background="?attr/selectableItemBackground"
        android:id="@+id/imageButton"
        android:layout_width="88dp"
        android:layout_height="87dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="31dp"
        android:layout_marginTop="154dp"
        app:srcCompat="@drawable/calculator" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="104dp"
        android:layout_height="103dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="141dp"
        android:background="?attr/selectableItemBackground"
        app:srcCompat="@drawable/torchpng" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="94dp"
        android:layout_height="81dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="26dp"
        android:background="?attr/selectableItemBackground"
        app:srcCompat="@drawable/btooth" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="92dp"
        android:layout_height="88dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="269dp"
        android:background="?attr/selectableItemBackground"
        app:srcCompat="@drawable/cam" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="101dp"
        android:layout_height="96dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="262dp"
        android:background="?attr/selectableItemBackground"
        app:srcCompat="@drawable/videocall" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="82dp"
        android:layout_height="85dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="274dp"
        android:layout_marginEnd="35dp"
        android:background="?attr/selectableItemBackground"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/videoplay" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="88dp"
        android:layout_height="85dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="23dp"
        android:layout_marginBottom="244dp"
        android:background="?attr/selectableItemBackground"
        app:srcCompat="@drawable/wifiicon" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="88dp"
        android:layout_height="77dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="248dp"
        android:background="?attr/selectableItemBackground"
        app:srcCompat="@drawable/vibrate" />

    <ImageButton
        android:background="?attr/selectableItemBackground"
        android:id="@+id/imageButton9"
        android:layout_width="96dp"
        android:layout_height="89dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="240dp"
        app:srcCompat="@drawable/music" />

    <ImageButton
        android:background="?attr/selectableItemBackground"
        android:id="@+id/imageButton10"
        android:layout_width="84dp"
        android:layout_height="86dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="242dp"
        android:layout_marginBottom="112dp"
        app:srcCompat="@drawable/internet" />

</RelativeLayout>